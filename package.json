{
  "name": "mulm-ai-playground",
  "version": "1.0.0",
  "description": "ÂµLM AI Workflow Playground - Build AI Systems Visually",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests specified yet'",
    "test:build": "npm run build && echo 'Build test passed'",
    "health-check": "npm run type-check && npm run lint --quiet && npm run build",
    "pre-demo": "npm run health-check && echo 'Ready for demo!'",
    "clean": "rm -rf dist node_modules/.vite .vite",
    "setup": "bash scripts/setup.sh",
    "build:prod": "bash scripts/build.sh",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "verify": "node scripts/master-verification.js",
    "verify:full": "node scripts/master-verification.js --demo",
    "verify:system": "node scripts/verify-system.js",
    "verify:performance": "node scripts/performance-benchmark.js",
    "verify:demo": "node scripts/demo-verification.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@types/react-resizable": "^3.0.8",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.292.0",
    "monaco-editor": "^0.52.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^7.8.2",
    "reactflow": "^11.11.4",
    "split-pane-react": "^0.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
